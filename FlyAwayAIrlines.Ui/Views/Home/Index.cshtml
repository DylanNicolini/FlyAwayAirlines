@{
    ViewBag.Title = "Flight Plan Management";
}
<link href="~/Content/toastr.css" rel="stylesheet"/>

<script src="~/Scripts/knockout-3.2.0.js"></script>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/KnockoutViewModels/FlightPlanViewModel.js"></script>
<script src ="~/KnockoutViewModels/DataFormatExtenders.js"></script>
<script src="~/Scripts/toastr.js"></script>

<div class="page-header">
    <h2 class="text-center">Flight Booking Management</h2>
</div>

<div class="row">
    <div class="col-md-8">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h2 class="panel-title">Available Flights</h2>
            </div>
            <div class="panel-body" data-bind="with: availableFlightsViewModel">
                @Html.Partial("_FlightPlanList")
            </div>
        </div>
    </div>

</div> 